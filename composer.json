{
    "name": "who/prime2",
    "description": "WHO Prime 2",
    "license": "proprietary",
	"config" : {
		"vendor-dir" : "protected/vendor",
		"bin-dir" : "protected/bin"
	},
	"minimum-stability" : "dev",
    "authors": [
        {
            "name": "Sam Mousa",
            "email": "sam@mousa.nl"
        },
        {
            "name": "Joey Claessen",
            "email": "joey@ch-ict.nl"
        }
    ],
    "require": {
		"php" : ">=5.6.0",
		"befound/yii-befound" : "dev-yii2",
        "kartik-v/yii2-widgets": "3.4.*",
        "kartik-v/yii2-builder": "1.*",
        "kartik-v/yii2-helpers": "1.*",
        "kartik-v/yii2-grid": "@dev",
        "kartik-v/yii2-mpdf": "@dev",
        "dektrium/yii2-user": "dev-master",
        "bariew/yii2-module-migration-controller": "1.*",
        "psr/http-message": "^1.0@dev",
        "bower-asset/bootstrap-sass": "3.*",
        "befound/yii2-limesurvey": "dev-master",
        "neomerx/json-api": "dev-master",
        "2amigos/yii2-ckeditor-widget" : "~1.0",
        "miloschuman/yii2-highcharts-widget": "dev-master",
        "guzzlehttp/psr7": "1.2.*"
    },
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.befound.nl"
		}
	],
    "autoload" : {
        "psr-4" : {
            "app\\" : ["protected"],
            "prime\\" : ["protected"]
        },
        "classmap" : [
            "protected/reportGenerators/"
        ]
    },
    "require-dev": {
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-debug": "~2.0@dev"
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library" : "protected/vendor/bower"
        }
    }
}

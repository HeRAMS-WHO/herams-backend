{
    "name": "who/prime2",
    "description": "WHO Prime 2",
    "license": "proprietary",
	"config" : {
		"vendor-dir" : "protected/vendor",
		"bin-dir" : "protected/bin"
	},
	"minimum-stability" : "stable",
    "authors": [
        {
            "name": "Sam Mousa",
            "email": "sam@mousa.nl"
        },
        {
            "name": "Joey Claessen",
            "email": "joey@ch-ict.nl"
        }
    ],
    "require": {
        "php" : ">=5.6.0",
        "ext-gd": "*",
        "befound/yii-befound" : "^2.0.0",
        "kartik-v/yii2-widgets": "^3.4.0",
        "kartik-v/yii2-builder": "^1.6",
        "kartik-v/yii2-helpers": "^1.3",
        "kartik-v/yii2-grid": "^3.0",
        "kartik-v/yii2-mpdf": "^1.0",
        "kartik-v/yii2-date-range" : "^1.6",
        "dektrium/yii2-user": "^0.9.9",
        "bariew/yii2-module-migration-controller": "1.*",
        "psr/http-message": "^1.0",
        "bower-asset/bootstrap-sass": "^3.3",
        "2amigos/yii2-ckeditor-widget" : "^1",
        "guzzlehttp/psr7": "^1.2",
        "bower-asset/is-loading": "^1",
        "bower-asset/bootbox": "^4.4",
        "miloschuman/yii2-highcharts-widget": "^4",
        "yiisoft/yii2": "^2.0.9",
        "treffynnon/navigator": "^1",
        "sam-it/ls2-jsonrpc-client": "^0.8.0",
        "dektrium/yii2-rbac": "^1.0.0-alpha",
        "npm-asset/mresize": "^1.0",
        "bower-asset/bootstrap-social": "^4.10",
        "bower-asset/jquery-locationpicker-plugin": "^0.1.12",
        "nesbot/carbon": "^1.21",
        "kartik-v/yii2-tabs-x": "^1.2.3 | @dev",
        "sam-it/yii2-magic": "^1.0.3",
        "primal/color": "dev-master",
        "lcobucci/jwt": "^3.2"
    },
	"repositories": [
		{
			"type": "composer",
			"url": "https://packages.sam-it.eu"
		}
	],
    "autoload" : {
        "psr-4" : {
            "app\\" : ["protected"],
            "prime\\" : ["protected"]
        },
        "classmap" : [
            "protected/reportGenerators/",
            "protected/Yii.php",
            "protected/vendor/yiisoft/yii2"
        ]
    },
    "require-dev": {
        "yiisoft/yii2-debug" : "*"
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library" : "protected/vendor/bower",
            "npm-asset-library" : "protected/vendor/bower"
        },
        "asset-repositories": [
          {
            "name": "npm-asset/data-exploration-tool",
            "type": "npm-vcs",
            "url": "git@gitlab.sam-it.eu:who/data-exploration-tool.git"
          }
        ]

    }
}

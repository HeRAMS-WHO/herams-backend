        "classmap" : [
            "vendor/yiisoft/yii2"
        ]
    },
    "require-dev": {
        "yiisoft/yii2-debug" : "2.0.x-dev",
        "codeception/codeception": "2.*",
        "lucidtaz/yii2-scssphp": "^0.2.1",
        "ifsnop/mysqldump-php": "^2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
